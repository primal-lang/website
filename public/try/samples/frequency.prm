frequency(list) = frequency.helper(list, {})

frequency.helper(list, result) =
    if (list.isEmpty(list))
        result
    else
        frequency.count(list.first(list), list.tail(list), result)

frequency.count(first, tail, result) =
    if (map.containsKey(result, first))
        frequency.helper(tail, map.set(result, first, result[first] + 1))
    else
        frequency.helper(tail, map.set(result, first, 1))

main = frequency([1, 2, 2, 3, 1, 4, 5, 2, 2, 5])